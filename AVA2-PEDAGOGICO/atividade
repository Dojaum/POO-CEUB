public class Atividade {
    private int id;
    private String titulo;
    private String descricao;
    private String tipo; 
    private int alunoId;

    public Atividade(int id, String titulo, String descricao, String tipo, int alunoId) {
        this.id = id;
        this.titulo = titulo;
        this.descricao = descricao;
        this.tipo = tipo;
        this.alunoId = alunoId;
    }

    
}
